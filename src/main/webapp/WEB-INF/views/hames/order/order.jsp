<div class="col-md-12">
	<h3 class="headline m-top-md">
		Order
		<span class="line"></span>
	</h3>
	<div class="panel panel-default no-margin">
		<div class="panel-heading">
			<a class="btn btn-success"><i class="fa fa-save"></i> Save</a>
			<a class="btn btn-danger"><i class="fa fa-trash-o"></i> Delete</a>
		</div>
		<div class="panel-body">
			<form class="form-horizontal">
				<div class="row">
					<div class="col-xs-4">
						<div class="form-group">
							<label for="jobId" class="col-lg-3 control-label">Job No</label>
							<div class="col-lg-9">
								<input type="text" class="form-control input-sm" placeholder="Job No">
							</div><!-- /.col -->
						</div><!-- /form-group -->
						<div class="form-group">
							<label for="jobName" class="col-lg-3 control-label">Job Name</label>
							<div class="col-lg-9">
								<input type="text" class="form-control input-sm" placeholder="Job Name">
							</div><!-- /.col -->
						</div><!-- /form-group -->
						<div class="form-group">
							<label for="jobName" class="col-lg-3 control-label">Job Description</label>
							<div class="col-lg-9">
								<textarea class="form-control input-sm" placeholder="Job Description"></textarea>
							</div><!-- /.col -->
						</div><!-- /form-group -->
					</div>
					<div class="col-xs-4">
						<div class="form-group">
							<label for="customerName" class="col-lg-3 control-label">Customer</label>
							<div class="col-lg-9">
								<input type="text" class="form-control input-sm" placeholder="Customer">
							</div><!-- /.col -->
						</div><!-- /form-group -->
					</div>
					<div class="col-xs-4">
						<div class="form-group">
							<label for="createdDate" class="col-lg-4 control-label">Created Date</label>
							<div class="col-lg-8">
								<input type="text" class="form-control input-sm" placeholder="Created Date">
							</div><!-- /.col -->
						</div><!-- /form-group -->
						<div class="form-group">
							<label for="deliveryDate" class="col-lg-4 control-label">Delivery Date</label>
							<div class="col-lg-8">
								<input type="text" class="form-control input-sm" placeholder="Delivery Date">
							</div><!-- /.col -->
						</div><!-- /form-group -->
					</div>
				</div>
				<hr />
				<strong>Printing</strong>
				<hr />
				<div class="row">
					<div class="col-xs-4">
						<div class="form-group">
							<label for="printingPaper" class="col-lg-3 control-label">Paper</label>
							<div class="col-lg-9">
								<input type="text" class="form-control input-sm" placeholder="Paper">
							</div><!-- /.col -->
						</div><!-- /form-group -->
						<div class="form-group">
							<label for="printingSize" class="col-lg-3 control-label">Size</label>
							<div class="col-lg-9">
								<input type="text" class="form-control input-sm" placeholder="Size">
							</div><!-- /.col -->
						</div><!-- /form-group -->
						<div class="form-group">
							<label for="printingQty" class="col-lg-3 control-label">Quantity</label>
							<div class="col-lg-9">
								<textarea class="form-control input-sm" placeholder="Quantity"></textarea>
							</div><!-- /.col -->
						</div><!-- /form-group -->
					</div>
				</div>
				<hr />
				<div class="row">
					<div class="col-xs-4">
						<div class="form-group">
							<label for="printing" class="col-lg-3 control-label">Printing</label>
							<div class="col-lg-9">
								<input type="text" class="form-control input-sm" placeholder="Printing">
							</div><!-- /.col -->
						</div><!-- /form-group -->
						<div class="form-group">
							<label for="numbering" class="col-lg-3 control-label">Numbering</label>
							<div class="col-lg-9">
								<input type="text" class="form-control input-sm" placeholder="Numbering">
							</div><!-- /.col -->
						</div><!-- /form-group -->
					</div>
					<div class="col-xs-4">
						<div class="form-group">
							<label for="stitching" class="col-lg-3 control-label">Stitching</label>
							<div class="col-lg-9">
								<input type="text" class="form-control input-sm" placeholder="Stitching">
							</div><!-- /.col -->
						</div><!-- /form-group -->
						<div class="form-group">
							<label for="binding" class="col-lg-3 control-label">Binding</label>
							<div class="col-lg-9">
								<input type="text" class="form-control input-sm" placeholder="Binding">
							</div><!-- /.col -->
						</div><!-- /form-group -->
						<div class="form-group">
							<label for="other" class="col-lg-3 control-label">Other</label>
							<div class="col-lg-9">
								<input type="text" class="form-control input-sm" placeholder="Other">
							</div><!-- /.col -->
						</div><!-- /form-group -->
					</div>
					<div class="col-xs-4">
						<div class="form-group">
							<label for="remarks" class="col-lg-3 control-label">Remarks</label>
							<div class="col-lg-9">
								<textarea class="form-control input-sm" placeholder="Remarks"></textarea>
							</div><!-- /.col -->
						</div><!-- /form-group -->
					</div>
				</div>
			</form>
		</div>
	</div>
</div>